{
  "name": "camp-project",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node ./dist/index",
    "build": "babel src --out-dir dist",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "start:dev:server": "nodemon --watch ./src --exec \"npm run build && npm start\"",
    "test": "nyc --reporter=html --reporter=text mocha --exit -r @babel/register ./__test__/**/*.spec.js && npm run coverage",
    "test:dev": "nodemon --watch ./__test__ --exec \"mocha --exit -r @babel/register ./__test__/**/*.spec.js\""
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.5.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/runtime": "^7.5.4",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "coveralls": "^3.0.5",
    "debug": "~2.6.9",
    "dotenv": "^8.0.0",
    "eslint-config-airbnb": "^17.1.1",
    "express": "~4.16.1",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^10.2.0",
    "morgan": "~1.9.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "pg": "^7.11.0",
    "supertest": "^4.0.2",
    "uuid": "^3.3.2",
    "validate.js": "^0.13.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
