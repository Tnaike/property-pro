    "babel-eslint": "^10.0.2",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.3",
    "make-runnable": "^1.3.6",
    "nodemon": "^1.19.1"
  },
